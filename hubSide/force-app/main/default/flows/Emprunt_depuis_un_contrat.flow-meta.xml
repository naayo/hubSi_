    <formulas>
        <name>EnvURL</name>
        <dataType>String</dataType>
        <expression>IF(
    CONTAINS({!$User.Username}, &apos;dev&apos;), 
    &apos;https://sfam--dev.lightning.force.com&apos;, 
    IF(
        CONTAINS({!$User.Username}, &apos;sit&apos;), 
        &apos;https://sfam--sit.lightning.force.com&apos;, 
        IF(
            CONTAINS({!$User.Username}, &apos;uat&apos;), 
            &apos;https://sfam--uat.lightning.force.com&apos;, 
            IF(
                CONTAINS({!$User.Username}, &apos;dev2&apos;), 
                &apos;https://sfam--dev2.lightning.force.com&apos;, 
                IF(
                    CONTAINS({!$User.Username}, &apos;pem&apos;), 
                    &apos;https://sfam--pem.lightning.force.com&apos;, 
                    &apos;https://sfam.my.salesforce.com&apos;
                )
            )
        )
    )
)</expression>
    </formulas>
